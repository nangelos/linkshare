{
  "manifest_version": 2,
  "name": "LinkShare",
  "version": "1.0.0",
  "description": "Send your current URL to your connections in Telegram",
  "icons": {
      "48": "icons/devtopocket-48.png"
  },
"browser_action": {
  "default_icon": {
    "48": "images/send.png"
  }
},
"background": {
    "scripts": ["background.js"]
},
  "permissions": [
    "storage",
    "cookies",
    "webNavigation",
    "<all_urls>",
    "tabs"
]
}
